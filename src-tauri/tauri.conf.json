{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/tooling/cli/schema.json",
  "identifier": "com.nugget.dev",
  "productName": "Nugget",
  "version": "0.1.0",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Nugget",
        "width": 800,
        "height": 600
      }
    ]
  },
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "plugins": {
    "audio": {
      "all": true
    },
    "network": {
      "all": true
    }
  }
}
